{
  "name": "webar-furniture-backend",
  "version": "1.0.0",
  "description": "WebAR Furniture Visualization Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:studio": "concurrently \"npm run dev\" \"npm run prisma:studio\"",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "ts-node src/scripts/migrate.ts",
    "migrate:prod": "node dist/scripts/migrate.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "keywords": [
    "webAR",
    "3D",
    "furniture",
    "echo3D"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "prisma": "^5.8.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
